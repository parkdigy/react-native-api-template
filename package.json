{
  "name": "express-api-template",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "dev": "concurrently --kill-others 'npm run dev:hot' 'npm run dev:lint'",
    "dev:hot": "npm run clean:dev && node-hot",
    "dev:lint": "nodemon --watch src --watch .env src/app.ts --exec 'eslint src --cache'",
    "lint": "eslint src",
    "clean:dev": "rm -rf @dev && mkdir @dev",
    "clean:prod": "rm -rf dist && mkdir dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress ",
    "build": "npm run watchman-del && npm run clean:prod && npm run test && npm run lint && npm run build:webpack",
    "watchman-del": "watchman watch-del \"${PWD}\" ; watchman watch-project \"${PWD}\"",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:restart": "pm2 delete ecosystem.config.js && pm2 start ecosystem.config.js",
    "pm2:reload": "pm2 reload ecosystem.config.js --force --update-env",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "install:prod": "npm install --only=prod",
    "reset-gitignore": "git rm -r --cached . && git add .",
    "reinstall-module": "rm -rf node_modules && rm -f package-lock.json && npm i",
    "make:dev": "git checkout main && git branch dev && git checkout dev && git push --set-upstream origin dev",
    "make:staging": "git checkout main && git branch staging && git checkout staging && git push --set-upstream origin staging",
    "make:publish:development": "git checkout dev && git branch publish/development && git checkout publish/development && sed -i '' 's/#PUB#//g' .gitignore && npm run reset-gitignore && git add . && git commit -m 'publish/development' && git push --set-upstream origin publish/development",
    "make:publish:staging": "git checkout staging && git branch publish/staging && git checkout publish/staging && sed -i '' 's/#PUB#//g' .gitignore && npm run reset-gitignore && git add . && git commit -m 'publish/staging' && git push --set-upstream origin publish/staging",
    "make:publish:production": "git checkout main && git branch publish/production && git checkout publish/production && sed -i '' 's/#PUB#//g' .gitignore && npm run reset-gitignore && git add . && git commit -m 'publish/production' && git push --set-upstream origin publish/production",
    "publish:development": "cross-env NODE_ENV=development node .publish.js",
    "publish:staging": "cross-env NODE_ENV=staging node .publish.js",
    "publish:production": "cross-env NODE_ENV=production node .publish.js",
    "pub:dev": "npm run publish:development",
    "pub:staging": "npm run publish:staging",
    "pub:prod": "npm run publish:production",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parkdigy/express-api-template.git"
  },
  "eslintConfig": {},
  "dependencies": {
    "express": "^4.18.2",
    "knex": "^3.1.0",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.523.0",
    "@aws-sdk/signature-v4-crt": "^3.523.0",
    "@pdg/util": "^1.0.20",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/csurf": "^1.11.5",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/flat": "^5.0.5",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/md5": "^2.3.5",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.21",
    "@types/node-schedule": "^2.1.6",
    "@types/nodemailer": "^6.4.14",
    "@types/url-join": "^4.0.3",
    "@types/uuid": "^9.0.8",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "aes256": "^1.1.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.2",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.1.0",
    "core-js": "^3.36.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "esbuild-loader": "^4.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "express-session": "^1.18.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "helmet": "^7.1.0",
    "iconv-lite": "^0.6.3",
    "jest": "^29.7.0",
    "js-big-decimal": "^2.0.7",
    "jsonwebtoken": "^9.0.2",
    "md5": "^2.3.0",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "node-hot-loader": "^1.21.10",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.10",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.1",
    "sheetjs-style": "^0.15.8",
    "ts-jest": "^29.1.2",
    "ts-mixer": "^6.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "url-safe-base64": "^1.3.0",
    "uuid": "^9.0.1",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.1/xlsx-0.20.1.tgz"
  }
}
