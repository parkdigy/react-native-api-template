    "@pdg/compare": "^1.0.8",
    "@pdg/crypto": "^1.0.5",
    "@pdg/data": "^1.0.5",
    "@pdg/date-time": "^1.0.4",
    "@pdg/formatting": "^1.0.8",
    "@pdg/masking": "^1.0.4",
    "@pdg/types": "^1.0.9",
    "@pdg/url": "^1.0.3",
    "@types/bcryptjs": "^3.0.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.22",
    "@types/express-session": "^1.18.1",
    "@types/flat": "^5.0.5",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mime-types": "^3.0.0",
    "@types/multer": "^1.4.12",
